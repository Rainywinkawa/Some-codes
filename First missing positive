//第一个缺失的正数
int firstMissingPositive(int* nums, int numsSize){
    int res[numsSize];
    for(int i = 0; i < numsSize; i++){
        r[i] = 0;
    }
    for(int i = 0; i < numsSize; i++){
        if(nums[i] > 0 && nums[i] <= numsSize){
            r[nums[i]-1] = 1;
        }
    }
    for(int i = 0; i < numsSize;i++){
        if(res[i] == 0){
            return i+1;
        }
    }
    return numsSize+1;
}
