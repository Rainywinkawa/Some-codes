//11.盛水最多的容器
int maxArea(int* height, int heightSize)
{
    int i=0,j=heightSize-1;
    int area=0,max=(j-i)*((height[i]<height[j])?height[i]:height[j]);
    while(j-i!=1)
    {
    if(height[i]<height[j])i++;
    else
    j--;
    area=(j-i)*((height[i]<height[j])?height[i]:height[j]);
    if(max<area)max=area;
    }
    return max;
}
