//无重复字符的最长字串
int lengthOfLongestSubstring(char* s) 
{
    int num=0,i=0,j=0,max=0;
    while(s[j]!='\0')
    {
        
        num=0,i=0;
        char set[127]={0};
        while(set[s[j+i]]!=s[j+i])
        {
            set[s[j+i]]=s[j+i];
            num++;i++;
        }
        if(num>max)max=num; 
        j++;
    } 
    return max;
