//跳跃游戏
bool canJump(int* nums, int numsSize)
{
    int health=nums[0],p,i=0,step=0;
    for(i=0;i<numsSize;i++)
    {
        p=nums[i];
        if(health<p)health=p;
        if(health>0)
        {
            step++;
            health--;
        }
        else 
        {
           if(step>=numsSize-1)return 1;   
           else return 0;
        } 
    }
    return 1;
}
